{
  "name": "ar-button",
  "private": true,
  "version": "0.0.1",
  "description": "awd",
  "source": "src/ar-button.ts",
  "module": "dist/ar-button.js",
  "types": "dist/types.d.ts",
  "type": "module",
  "targets": {
    "default": {
      "distDir": "./dist"
    }
  },
  "scripts": {
    "serve": "parcel ./index.html --port 8080 --target default",

    "watch": "parcel watch --target default",

    "build:prod": "parcel build --target default",
    "build:dev": "NODE_ENV=development parcel build --target default",

    "deploy:dev": "gsutil rm -a gs://ar-button-dev/ar-button/** && gsutil cp -z js dist/*.js gs://ar-button-dev/ar-button/ && gsutil setmeta -h \"Cache-Control: no-store, no-cache\" gs://ar-button-dev/ar-button/*"
  },
  "author": "Bitforge AG",
  "license": "MIT",
  "dependencies": {
    "@parcel/transformer-typescript-tsc": "^2.5.0",
    "@swc/helpers": "^0.3.13",
    "@types/qrcode": "^1.4.2",
    "lit": "^2.1.0",
    "qr-code-styling": "^1.6.0-rc.1",
    "typescript": "^4.6.4",
    "watch": "^1.0.2"
  },
  "devDependencies": {
    "@parcel/transformer-typescript-types": "2.5.0",
    "@web/dev-server": "^0.1.22",
    "@web/dev-server-legacy": "^0.1.4",
    "@web/dev-server-rollup": "^0.3.9",
    "@web/test-runner": "^0.13.16",
    "@web/test-runner-mocha": "^0.7.4",
    "@web/test-runner-playwright": "^0.8.4",
    "@webcomponents/webcomponentsjs": "^2.6.0",
    "parcel": "^2.5.0",
    "prettier": "^2.3.2"
  },
  "customElements": "custom-elements.json"
}
